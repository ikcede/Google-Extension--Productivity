{
  "name": "Productive.ly",
  "version": "0.3.1",
  "manifest_version": 2,
  "description": "Prototype for keeping yourself on track.",
  "icons": {
  	 "16": "16.png",
  	 "48": "48.png",
  	"128": "128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "iconminus.png",
	"default_popup": "popupmain.html"
  },
  "permissions": [
	"tabs", "http://*/*", "background", "https://*/*", "*://*/*"]
}